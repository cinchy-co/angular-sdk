<?xml version="1.0" encoding="utf-16"?>
<Model xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="SDK Demo Model" version="1.0.0" xmlns="http://www.cinchy.co">
  <Schema>
    <Tables>
      <Table name="Answers" domain="SDK Demo" guid="" icon="fa fa-adn" iconColour="#00B050">
        <Columns>
          <Text name="Answer" guid="" isMandatory="true" />
          <Link name="Question" guid="" isMandatory="true" linkedTableDomain="SDK Demo" linkedTable="Questions">
            <SearchDisplayColumns>
              <LinkedColumn isPrimary="true">Cinchy Id</LinkedColumn>
            </SearchDisplayColumns>
          </Link>
          <Link name="User" guid="" isMandatory="true" linkedTableDomain="SDK Demo" linkedTable="Users">
            <SearchDisplayColumns>
              <LinkedColumn isPrimary="true">Name</LinkedColumn>
            </SearchDisplayColumns>
          </Link>
        </Columns>
        <UniqueConstraints />
      </Table>
      <Table name="Questions" domain="SDK Demo" guid="" icon="fa fa-question-circle" iconColour="#00B050">
        <Columns>
          <Text name="Question" guid="" isMandatory="true" />
          <Text name="Location" guid="" />
          <Link name="Asker" guid="" linkedTableDomain="SDK Demo" linkedTable="Users">
            <SearchDisplayColumns>
              <LinkedColumn isPrimary="true">Name</LinkedColumn>
            </SearchDisplayColumns>
          </Link>
        </Columns>
        <UniqueConstraints />
      </Table>
      <Table name="Users" domain="SDK Demo" guid="" icon="fa fa-users" iconColour="#00B050">
        <Columns>
          <Link name="Name" guid="" linkedTableDomain="Cinchy" linkedTable="Users">
            <SearchDisplayColumns>
              <LinkedColumn isPrimary="true">Username</LinkedColumn>
            </SearchDisplayColumns>
          </Link>
        </Columns>
        <UniqueConstraints />
      </Table>
      <Table name="Users" domain="Cinchy" guid="" icon="fa fa-user" iconColour="#00B050" description="System table for maintaining users">
        <Columns>
          <Text name="Username" guid="" isMandatory="true" maxLength="100" />
          <Choice name="Authentication Method" guid="" isMandatory="true" allowLinking="false" allowDisplayInLinkedViews="false">
            <Option>Cinchy User Account</Option>
            <Option>Single Sign-On</Option>
            <Option>Non Interactive</Option>
          </Choice>
          <Text name="Password" guid="" isMandatory="true" allowLinking="false" allowDisplayInLinkedViews="false" maxLength="100" />
          <Text name="Email Address" guid="" isMandatory="true" allowLinking="false" maxLength="100" />
          <Text name="Email Verification Code" guid="" allowLinking="false" allowDisplayInLinkedViews="false" maxLength="100" />
          <YesNo name="Email Verified" guid="" allowLinking="false" allowDisplayInLinkedViews="false" />
          <Text name="First Name" guid="" isMandatory="true" allowLinking="false" allowDisplayInLinkedViews="false" maxLength="100" />
          <Text name="Last Name" guid="" allowLinking="false" allowDisplayInLinkedViews="false" maxLength="100" />
          <Calculated name="Name" guid="" allowLinking="false" calculationExpression="CONCAT([First Name], (CASE WHEN [Last Name] is NULL or LTRIM([Last Name]) = '' THEN '' ELSE ' ' END), ISNULL([Last Name], ''))" outputFormat="Text" outputFieldLength="205" />
          <Calculated name="Display Name" guid="" calculationExpression="CONCAT([First Name], (CASE WHEN [Last Name] is NULL or LTRIM([Last Name]) = '' THEN '' ELSE ' ' END), ISNULL([Last Name], ''), ' (', [Username], ')')" outputFormat="Text" outputFieldLength="305" />
          <Binary name="Profile Photo" guid="" allowLinking="false" allowDisplayInLinkedViews="false" />
          <Date name="Password Expiration Timestamp" guid="" allowLinking="false" allowDisplayInLinkedViews="false" />
          <YesNo name="Is Disabled" guid="" allowLinking="false" allowDisplayInLinkedViews="false" defaultValue="false" />
          <Date name="Locked Timestamp" guid="" allowLinking="false" allowDisplayInLinkedViews="false" />
          <YesNo name="Can Design Tables" guid="" allowLinking="false" allowDisplayInLinkedViews="false" defaultValue="true" />
        </Columns>
        <UniqueConstraints>
          <UniqueConstraint name="UC_Username">
            <Column>Username</Column>
          </UniqueConstraint>
        </UniqueConstraints>
      </Table>
    </Tables>
  </Schema>
</Model>