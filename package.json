{
  "name": "angular-sdk-wrapper",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/cinchy-co/angular-sdk"
  },
  "author": {
    "name": "Cinchy",
    "email": "opensource@cinchy.co"
  },
  "keywords": [
    "angular",
    "cinchy",
    "api",
    "data network",
    "data collabortation"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cinchy-co/angular-sdk/issues"
  },
  "scripts": {
    "ng": "ng",
    "build": "ng build angular-sdk",
    "build:clean": "npm run clean && npm install --force && npm run build",
    "clean": "npm run clean:windows:dist && npm run clean:windows:node_modules && npm run clean:unix",
    "clean:windows:dist": "node -e \"if (process.platform === 'win32') process.exit(1)\" || , (if exist dist rmdir /Q /S dist && if exist node_modules rmdir /Q /S node_modules)",
    "clean:windows:node_modules": "node -e \"if (process.platform === 'win32') process.exit(1)\" || , if exist node_modules rmdir /Q /S node_modules",
    "clean:unix": "node -e \"if (process.platform !== 'win32') process.exit(1)\" || rm -rf dist node_modules",
    "watch": "ng build angular-sdk --watch --configuration development"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "angular-oauth2-oidc": "^13.0.1",
    "iframe-resizer": "^4.3.2",
    "jsrsasign": "^10.8.6",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "util": "^0.12.4",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.0",
    "@angular-devkit/core": "~12.2.0",
    "@angular/cli": "~12.2.0",
    "@angular/compiler-cli": "~12.2.0",
    "@angular/language-service": "~12.2.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "ng-packagr": "^12.1.1",
    "ts-node": "~8.3.0",
    "typescript": "~4.3.5"
  }
}
